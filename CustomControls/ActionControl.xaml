<UserControl x:Class="FusionX_Editor.CustomControls.ActionControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:FusionX_Editor.CustomControls"
             mc:Ignorable="d"
             d:DesignHeight="25" d:DesignWidth="29">
    
    <Border BorderBrush="#a0a0a0" BorderThickness="0,0,1,1" Width="29" Height="25">
        
        <Button Background="Transparent" BorderThickness="0">
            <Button.Content>
            <Image Name="Checkmark" Source="../Resources/EE_Checkmark.png"></Image>
            </Button.Content>
        </Button>
    </Border>
    
</UserControl>
